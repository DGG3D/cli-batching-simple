{
  "optimize": {
    "3dModelOptimizationMethod": {
      "meshAndMaterialOptimization": {
        "decimator": {
          "target": {
            "deviation": {
              "percentage": 0.01
            }
          },
          "materialOptimization": {
            "materialMerger": {
              "materialRegenerator": {
                "uvAtlasGenerator": {
                  "textureBaker": {
                    "normalMap": {}
                  }
                }
              }
            }
          },
          "collapseDistanceThreshold": 0.01
        }
      }
    }
  },
  "export": [
    {
      "textureMapFilePrefix": "",
      "discard": {},
      "format": {
        "gltf": {
          "pbrMaterial": {}
        }
      }
    }
  ]
}
